<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <RootNamespace>aks_middleware_csharp</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>ADuke.ServiceHub.AKSMiddleware</PackageId>
    <Version>2.0.0</Version>
    <Author>ADuke</Author>
    <Product>AKSMiddleware</Product>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Google.Protobuf" Version="3.27.2" />
    <PackageReference Include="Grpc.Core" Version="2.46.6" />
    <PackageReference Include="Microsoft.Extensions.Logging" Version="8.0.0" />
    <PackageReference Include="Polly" Version="8.4.1" />
    <PackageReference Include="ProtoValidate" Version="0.1.3" />
    <PackageReference Include="Serilog" Version="4.0.0" />
    <PackageReference Include="Serilog.Extensions.Logging" Version="8.0.0" />
    <PackageReference Include="ServiceHub.MyGreeterV3.LogProto" Version="3.9.5" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="LogProto\**\*" />
  </ItemGroup>

</Project>
